{"code":"import { __awaiter } from \"tslib\";\nimport { input, select } from '@inquirer/prompts'; //终端交互库\nimport { clone } from '../utils/clone';\nexport const templates = new Map([\n    [\n        'Vite-Vue3-Typescript-tempalte--完整版',\n        {\n            name: 'Vite-Vue3-Typescript-tempalte-dev5',\n            downloadUrl: 'https://gitlab.com/wangderful-group/dawei-engineerProject.git',\n            description: '完整模板',\n            branch: 'dev5',\n        },\n    ],\n    [\n        'Vite-Vue3-开发版',\n        {\n            name: 'Vite-Vue3-Typescript-tempalte-dev2',\n            downloadUrl: 'https://gitlab.com/wangderful-group/dawei-engineerProject.git',\n            description: '开发版',\n            branch: 'dev2',\n        },\n    ],\n]);\nexport function create(projectName) {\n    return __awaiter(this, void 0, void 0, function* () {\n        // 初始化模板列表\n        const templateList = Array.from(templates).map((item) => {\n            const [name, info] = item;\n            return {\n                name,\n                value: name,\n                description: info.description,\n            };\n        });\n        if (!projectName) {\n            projectName = yield input({ message: '请输入项目名称' });\n        }\n        const templateName = yield select({\n            message: '请选择模板',\n            choices: templateList,\n        });\n        const info = templates.get(templateName);\n        console.log(info);\n        if (info) {\n            clone(info.downloadUrl, projectName, ['-b', info.branch]);\n        }\n    });\n}\n","references":["D:/Desktop/脚手架/my-vue-cli/node_modules/@inquirer/prompts/dist/cjs/types/index.d.ts","D:/Desktop/脚手架/my-vue-cli/src/utils/clone.ts"]}
